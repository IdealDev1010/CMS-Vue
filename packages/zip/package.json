{
  "name": "@pruvious/zip",
  "version": "1.0.3",
  "description": "Un(zip) tool for Pruvious packages.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "types/index.d.ts",
  "scripts": {
    "start": "node dist/index.cjs",
    "build": "node scripts/build.js",
    "dev": "node scripts/build.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pruvious/pruvious.git"
  },
  "author": "Muris Ceman <hello@pruvious.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pruvious/pruvious/issues"
  },
  "homepage": "https://pruvious.com",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.10.0"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@pruvious/build": "^1.0.3",
    "@types/adm-zip": "^0.5.0",
    "@types/fs-extra": "^9.0.13"
  }
}
