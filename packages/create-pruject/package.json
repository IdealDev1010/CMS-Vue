{
  "name": "create-pruject",
  "version": "1.0.1",
  "description": "The simple way to develop and deploy websites using Pruvious and Nuxt.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "create-pruject": "dist/index.cjs"
  },
  "types": "types/index.d.ts",
  "scripts": {
    "start": "node dist/index.cjs",
    "build": "node scripts/build.js",
    "dev": "node scripts/build.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pruvious/pruvious.git"
  },
  "author": "Muris Ceman <hello@pruvious.com>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/pruvious/pruvious/issues"
  },
  "homepage": "https://pruvious.com",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.10.0"
  },
  "dependencies": {
    "@pruvious/build": "^1.0.1",
    "@pruject/dev": "^1.0.1",
    "@pruvious/utils": "^1.0.1",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13"
  }
}
