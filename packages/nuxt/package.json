{
  "name": "@pruvious/nuxt",
  "version": "1.0.2",
  "description": "Pruvious + Nuxt coupling tool.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground --no-clear",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pruvious/pruvious.git"
  },
  "author": "Muris Ceman <hello@pruvious.com>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/pruvious/pruvious/issues"
  },
  "homepage": "https://pruvious.com",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.10.0"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0",
    "@pruvious/build": "^1.0.2",
    "@pruvious/shared": "^1.0.2",
    "@pruvious/utils": "^1.0.2",
    "axios": "^1.3.5",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/prettier": "^2.7.2",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.29.0",
    "nuxt": "^3.0.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4"
  }
}
