{
  "name": "@pruject/dev",
  "version": "1.0.2",
  "description": "The simple way to develop and deploy websites using Pruvious and Nuxt.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "pruject": "dist/bin.cjs"
  },
  "types": "types/index.d.ts",
  "scripts": {
    "start": "node dist/index.cjs",
    "build": "node scripts/build.js",
    "dev": "node scripts/build.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pruvious/pruvious.git"
  },
  "author": "Muris Ceman <hello@pruvious.com>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/pruvious/pruvious/issues"
  },
  "homepage": "https://pruvious.com",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.10.0"
  },
  "dependencies": {
    "@pruvious/build": "^1.0.2",
    "@pruvious/utils": "^1.0.2",
    "@pruvious/zip": "^1.0.2",
    "args": "^5.0.3",
    "dayjs": "^1.11.7",
    "detect-port": "^1.5.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.1.0",
    "inquirer": "^8.2.5",
    "kill-port": "^2.0.1",
    "node-ssh": "^13.1.0",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "semver": "^7.5.0",
    "sshpk": "^1.17.0",
    "terminal-kit": "^3.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/sshpk": "^1.17.1",
    "@types/terminal-kit": "^2.5.1",
    "default-browser": "^4.0.0",
    "define-lazy-prop": "^3.0.0",
    "is-inside-container": "^1.0.0",
    "is-wsl": "^2.2.0"
  }
}
