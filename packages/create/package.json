{
  "name": "create-pruvious",
  "version": "1.0.0",
  "description": "Boilerplate to create a new Pruvious project.",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "create-pruvious": "dist/index.cjs"
  },
  "types": "types/index.d.ts",
  "scripts": {
    "start": "node dist/index.cjs",
    "build": "node scripts/build.js",
    "dev": "node scripts/build.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pruvious/pruvious.git"
  },
  "author": "Muris Ceman <hello@pruvious.com>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/pruvious/pruvious/issues"
  },
  "homepage": "https://pruvious.com",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.10.0"
  },
  "dependencies": {
    "@pruvious/utils": "^1.0.0",
    "fs-extra": "^11.1.1",
    "picocolors": "^1.0.0",
    "prompt-sync": "^4.2.0"
  },
  "devDependencies": {
    "@pruvious/build": "^1.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/prompt-sync": "^4.2.0"
  }
}
