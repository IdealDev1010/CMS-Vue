{
  "name": "pruvious",
  "version": "3.9.10",
  "description": "A reliable CMS for your Nuxt app.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pruvious/pruvious.git"
  },
  "author": "Muris Ceman <hello@pruvious.com>",
  "license": "MIT",
  "main": "./dist/module.cjs",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs"
    },
    "./config": {
      "import": "./config/index.ts"
    }
  },
  "types": "./dist/types.d.ts",
  "files": [
    "bin",
    "config",
    "dist"
  ],
  "bin": {
    "pruvious": "./bin/index.js"
  },
  "scripts": {
    "build": "node ./scripts/build.js",
    "dev": "node ./scripts/dev.js",
    "dev:build": "nuxi build playground",
    "dev:pg": "cross-env NUXT_PRUVIOUS_DATABASE=postgresql://pruvious:12345678@127.0.0.1:5432/pruvious node ./scripts/dev.js",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "dev:redis": "cross-env NUXT_PRUVIOUS_REDIS=redis://127.0.0.1:6379/0 node ./scripts/dev.js",
    "dev:s3": "cross-env NUXT_PRUVIOUS_UPLOADS_DRIVE_TYPE=s3 NUXT_PRUVIOUS_UPLOADS_DRIVE_KEY=pruvious NUXT_PRUVIOUS_UPLOADS_DRIVE_SECRET=pruvious NUXT_PRUVIOUS_UPLOADS_DRIVE_BUCKET=pruvious NUXT_PRUVIOUS_UPLOADS_DRIVE_REGION=pruvious NUXT_PRUVIOUS_UPLOADS_DRIVE_ENDPOINT=http://127.0.0.1:9000 NUXT_PRUVIOUS_UPLOADS_DRIVE_BASE_URL=http://127.0.0.1:9000/pruvious node ./scripts/dev.js",
    "format": "prettier --write .",
    "lint": "eslint .",
    "release": "node ./scripts/release.js",
    "test": "vitest run",
    "test:pg": "cross-env NUXT_PRUVIOUS_DATABASE=postgresql://pruvious:12345678@127.0.0.1:5432/pruvious_test vitest run",
    "test:redis": "cross-env NUXT_PRUVIOUS_REDIS=redis://127.0.0.1:6379/1 vitest run",
    "test:s3": "cross-env NUXT_PRUVIOUS_UPLOADS_DRIVE_TYPE=s3 NUXT_PRUVIOUS_UPLOADS_DRIVE_KEY=pruvious NUXT_PRUVIOUS_UPLOADS_DRIVE_SECRET=pruvious NUXT_PRUVIOUS_UPLOADS_DRIVE_BUCKET=pruvious NUXT_PRUVIOUS_UPLOADS_DRIVE_REGION=pruvious NUXT_PRUVIOUS_UPLOADS_DRIVE_ENDPOINT=http://127.0.0.1:9000 NUXT_PRUVIOUS_UPLOADS_DRIVE_BASE_URL=http://127.0.0.1:9000/pruvious vitest run",
    "test-ui": "vitest --ui",
    "test-ui:pg": "cross-env NUXT_PRUVIOUS_DATABASE=postgresql://pruvious:12345678@127.0.0.1:5432/pruvious_test vitest --ui",
    "test-ui:redis": "cross-env NUXT_PRUVIOUS_REDIS=redis://127.0.0.1:6379/1 vitest --ui",
    "test-ui:s3": "cross-env NUXT_PRUVIOUS_UPLOADS_DRIVE_TYPE=s3 NUXT_PRUVIOUS_UPLOADS_DRIVE_KEY=pruvious NUXT_PRUVIOUS_UPLOADS_DRIVE_SECRET=pruvious NUXT_PRUVIOUS_UPLOADS_DRIVE_BUCKET=pruvious NUXT_PRUVIOUS_UPLOADS_DRIVE_REGION=pruvious NUXT_PRUVIOUS_UPLOADS_DRIVE_ENDPOINT=http://127.0.0.1:9000 NUXT_PRUVIOUS_UPLOADS_DRIVE_BASE_URL=http://127.0.0.1:9000/pruvious vitest --ui",
    "test:watch": "vitest watch",
    "try": "node ./scripts/try.js",
    "typecheck": "nuxi typecheck"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.7",
    "@aws-sdk/client-s3": "^3.478.0",
    "@babel/generator": "^7.23.6",
    "@inquirer/editor": "^1.2.13",
    "@nuxt/kit": "^3.9.3",
    "@tiptap/core": "2.1.13",
    "@tiptap/extension-highlight": "2.1.13",
    "@tiptap/extension-link": "2.1.13",
    "@tiptap/extension-placeholder": "2.1.13",
    "@tiptap/extension-subscript": "2.1.13",
    "@tiptap/extension-superscript": "2.1.13",
    "@tiptap/extension-text-align": "2.1.13",
    "@tiptap/extension-text-style": "2.1.13",
    "@tiptap/extension-underline": "2.1.13",
    "@tiptap/pm": "2.1.13",
    "@tiptap/starter-kit": "2.1.13",
    "@vueuse/integrations": "^10.7.0",
    "accept-language-parser": "^1.5.0",
    "argon2": "^0.31.2",
    "c12": "^1.5.1",
    "citty": "^0.1.5",
    "colorette": "^2.0.20",
    "consola": "^3.2.3",
    "dayjs": "^1.11.10",
    "defu": "^6.1.3",
    "dompurify": "^3.0.6",
    "dot-prop": "^8.0.2",
    "execa": "^7.2.0",
    "flatpickr": "^4.6.13",
    "focus-trap": "^7.5.4",
    "fs-extra": "^11.2.0",
    "giget": "^1.2.1",
    "jiti": "^1.21.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^3.1.2",
    "listhen": "^1.5.5",
    "magicast": "^0.2.11",
    "mime": "^3.0.0",
    "nanoid": "^4.0.2",
    "node-ssh": "^13.1.0",
    "nypm": "^0.3.3",
    "ohash": "^1.1.3",
    "pathe": "^1.1.1",
    "perfect-debounce": "^1.0.0",
    "pg": "^8.11.3",
    "pg-connection-string": "^2.6.2",
    "pluralize-esm": "^9.0.5",
    "redis": "^4.6.12",
    "scule": "1.1.1",
    "semver": "^7.5.4",
    "sequelize": "^6.35.2",
    "sharp": "^0.32.6",
    "sortablejs": "^1.15.2",
    "sqlite3": "^5.1.6",
    "std-env": "^3.7.0",
    "tar": "^6.2.0",
    "tippy.js": "^6.3.7",
    "ufo": "^1.3.2",
    "vue-slider-component": "4.1.0-beta.7"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "3.9.3",
    "@nuxt/test-utils": "^3.10.0",
    "@types/accept-language-parser": "^1.5.6",
    "@types/babel__generator": "^7.6.8",
    "@types/bytes": "^3.1.4",
    "@types/dompurify": "^3.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mime": "^3.0.4",
    "@types/ms": "^0.7.34",
    "@types/node": "^18.19.3",
    "@types/pg": "^8.10.9",
    "@types/pluralize": "^0.0.30",
    "@types/tar": "^6.1.10",
    "@vitest/ui": "^0.34.7",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.10",
    "eslint": "^8.56.0",
    "nuxt": "^3.9.2",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.4.0",
    "vitest": "^0.34.0"
  },
  "packageManager": "pnpm@8.6.6",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20"
  }
}
